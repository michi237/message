</head>
<body>
<!--NAV-->
<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <div class="container">
        <a class="navbar-brand" href="/message/public">Message writer</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarColor02">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href="/message/public">Strona główna</a>
                </li>
                {% if app.user %}
                <li class="nav-item">
                    <a class="nav-link" href="/message/public/message">Wiadomości</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/message/public/view">Podgląd</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/message/public/logout">Wyloguj się</a>
                </li>
                {% else %}
                <li class="nav-item">
                    <a class="nav-link" href="/message/public/login">Zaloguj się</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/message/public/register">Zarejestruj się</a>
                </li>
                {% endif %}
            </ul>
        </div>
    </div>
</nav>
{% block body %}

{% endblock %}
